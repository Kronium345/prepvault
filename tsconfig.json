{
  "compilerOptions": {
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "target": "es2016",
    "module": "commonjs",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "react",
      "react-native"
    ],
    "baseUrl": "./",
    "paths": {
      "*": [
        "*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}